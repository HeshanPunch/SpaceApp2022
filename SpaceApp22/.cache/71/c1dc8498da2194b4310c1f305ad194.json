gb(\n        wave(0, 255, t),\n        wave(0, 255, t + 2),\n        wave(0, 255, t + 4),\n      )\n      btn.scale = vec2(1.2)\n    } else {\n      btn.scale = vec2(1)\n      btn.color = rgb()\n    }\n  })\n}\n\nscene(\"start\", () => {\n  addButton(\"Start\", vec2(k.width() * 0.5, k.height() * 0.5), () => k.go('inputName'));\n})\n\nscene(\"inputName\", () => {\n  addButton(\"Username\", vec2(k.width() * 0.5, k.height() * 0.5), () => k.go('mainGame'));\n})\n\nscene('mainGame', () => {\n\n  const WALL = [\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n    '===============================',\n  ]\n\n  const level = addLevel(WALL, {\n    width: 64,\n    height: 64,\n    \"=\": () => [\n      sprite(\"wall\"),\n      area(),\n      solid(),\n    ],\n  })\n})\n\nk.go('start')\n\n"},"lineCount":null}},"error":null,"hash":"3d8dafb09d55d6217a6613e517b74ecf","cacheData":{"env":{}}}